<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <%include header%>
    <article>
      <h1><%= title %></h1>
      <p>Welcome to <%= title %></p>
      <%
        if(docs) {
          docs.forEach(function(doc){%>
            <%=doc.mobile+"------"+doc.post%><br/>
      <%});
        }
      %>
      
      <form method="post" action="/">
        <input type="text" name="message"/>
        <button type="submit">提交</button>
      </form>

    </article>
    <%include footer%>
  </body>
</html>
